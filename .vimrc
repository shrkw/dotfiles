filetype off

augroup default
  autocmd!
augroup END

" encoding -----------------------------------------------------------
"set encoding=utf-8
"set fileencodings=utf-8,utf-16,ucs-2,cp932,euc-jp,iso-2022-jp

filetype plugin indent on

source ~/.vim/.vimrc.basic
source ~/.vim/.vimrc.edit
source ~/.vim/.vimrc.func_and_keymap

syntax enable

